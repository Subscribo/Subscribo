{
    "name": "subscribo/support",
    "description": "Support package with common shared functionality (Str, Arr) extending Laravel classes",
    "keywords": [
        "laravel",
        "subscribo",
        "support"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "illuminate/support": "~5.0"
    },
    "suggest": {
        "laravel/framework": "This package have been developed to work with The Laravel Framework 5.0"
    },
    "autoload": {
