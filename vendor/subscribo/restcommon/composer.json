{
    "name": "subscribo/restcommon",
    "description": "Package containing common files for Subscribo REST client and server",
    "keywords": [
        "laravel",
        "subscribo",
        "common",
        "rest",
        "access token",
        "header field name",
        "exception",
        "authentication"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "KochAbo GmbH",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "subscribo/exception": "@dev",
        "subscribo/support": "@dev",
        "illuminate/contracts": "~5.0",
        "illuminate/encryption": "~5.0",
        "symfony/http-foundation": "~2.6"
    },
    "suggest": {
        "laravel/framework": "This package have been developed to work with The Laravel Framework 5.0"
    },
    "autoload": {
        "psr-4": {
            "Subscribo\\RestCommon\\": "src/Subscribo/RestCommon/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
