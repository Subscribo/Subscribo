{
    "name": "subscribo/serviceprovider",
    "description": "Contain Service Provider for Laravel to be extended by other packages as well as modified versions of Service Provider from Laravel 4.2.12",
    "keywords": [
        "laravel",
        "subscribo",
        "transition"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "illuminate/support": "~5.0"
    },
    "suggest": {
        "laravel/framework": "This package have been developed to work with The Laravel Framework 5.0"
    },
    "autoload": {
        "psr-4": {
            "Subscribo\\ServiceProvider\\": "src/Subscribo/ServiceProvider/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
