{
    "name": "subscribo/schemabuilder",
    "description": "Schema builder for building models, migrations, etc from schema",
    "keywords": [
        "laravel",
        "subscribo",
        "schema"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "symfony/yaml": "~2.6@stable",
        "subscribo/dependencyresolver": "@dev",
        "subscribo/serviceprovider": "@dev"
    },
    "suggest": {
        "laravel/framework": "This package have been build to work with The Laravel Framework version 4.2. Version 5.0 can also be used.",
        "frozennode/administrator": "This package generates config files for Frozennode Administrator"

    },
    "autoload": {
        "psr-4": {
            "Subscribo\\SchemaBuilder\\": "src/Subscribo/SchemaBuilder/",
            "Fuel\\Core\\": "src/lib/fuel/core/src/Fuel/Core/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
