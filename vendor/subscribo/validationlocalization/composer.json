{
    "name": "subscribo/validationlocalization",
    "description": "Provides localization of Laravel Validator using Subscribo Localization Localizer",
    "keywords": [
        "laravel",
        "subscribo",
        "localization",
        "localisation",
        "localizer",
        "validation",
        "validator"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "illuminate/validation": "~5.0",
        "subscribo/localization": "@dev",
        "subscribo/support": "@dev"
    },
    "suggest": {
        "laravel/framework": "This package have been developed to work with The Laravel Framework 5.0"
    },
    "autoload": {
        "psr-4": {
            "Subscribo\\ValidationLocalization\\": "src/Subscribo/ValidationLocalization/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
