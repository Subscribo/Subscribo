{
    "name": "subscribo/restclient",
    "description": "Package Subscribo REST Client for communicating with Subscribo API",
    "keywords": [
        "laravel",
        "subscribo",
        "REST",
        "http",
        "client"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "subscribo/restcommon": "@dev",
        "subscribo/exception": "@dev",
        "subscribo/config": "@dev",
        "subscribo/environment": "@dev",
        "subscribo/support": "@dev",
        "symfony/http-foundation": "~2.6",
        "guzzlehttp/guzzle": "~5.0"
    },
    "suggest": {
        "laravel/framework": "This package have been developed to work with The Laravel Framework 5.0",
        "subscribo/serviceprovider": "If using ServiceProvider for Laravel"
    },
    "autoload": {
        "psr-4": {
            "Subscribo\\RestClient\\": "src/Subscribo/RestClient/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
