    "keywords": [
        "laravel",
        "subscribo",
        "billing",
        "charging",
        "payment"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "KochAbo GmbH",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "subscribo/support": "@dev",
        "subscribo/transaction-plugin-klarna": "@dev",
        "subscribo/transaction-plugin-payunity": "@dev"
    },
    "suggest": {
        "laravel/framework": "This package have been developed to work with The Laravel Framework 5.0"
    },
    "autoload": {
        "psr-4": {
            "Subscribo\\Billing\\": "src/Subscribo/Billing/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
