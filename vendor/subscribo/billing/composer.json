        "laravel",
        "subscribo",
        "billing",
        "charging",
        "payment"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
