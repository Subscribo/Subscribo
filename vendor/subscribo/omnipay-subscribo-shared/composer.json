    "description": "Package with shared functionality for some Omnipay drivers",
    "keywords": [
        "Omnipay",
        "driver"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner",
            "homepage": "http://kochabo.com"
        }
    ],
    "require": {
        "omnipay/common": ">=2.0",
        "egeloen/http-adapter": "*",
        "psr/http-message": "*@dev"
