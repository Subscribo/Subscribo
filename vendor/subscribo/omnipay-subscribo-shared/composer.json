{
    "name": "subscribo/omnipay-subscribo-shared",
    "description": "Package with shared functionality for some Omnipay payment processing library drivers",
    "keywords": [
        "Omnipay",
        "driver",
        "shared"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "KochAbo GmbH",
            "role": "owner"
        }
    ],
    "support": {
        "email": "packages@subscribo.io",
        "issues": "https://github.com/Subscribo/omnipay-subscribo-shared/issues",
        "source": "https://github.com/Subscribo/omnipay-subscribo-shared"
    },
    "require": {
        "php": ">=5.4",
        "omnipay/common": "^2.3.2"
    },
    "require-dev": {
        "omnipay/tests": "^2.0",
        "phpunit/phpunit": "<5.0",
        "egeloen/http-adapter": "^0.8@dev",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0",
        "symfony/http-foundation": "<=3.0",
        "subscribo/psr-http-message-tools": ">=0.3.2 <0.5",
        "phly/http": ">=0.13.3 <0.15.0"
    },
    "suggest": {
        "subscribo/psr-http-message-tools": "(>=0.3.2 <0.5) For PSR-7 compliant Request object factory, e.g. when SimpleRestfulRequestTrait, AbstractRestfulResponse or SimpleRestfulResponse is used",
        "psr/log": "(^1.0) When GuzzleClientHelper or AbstractGateway::attachPsrLogger() is used",
        "egeloen/http-adapter": "(^0.8@dev) when HttpMessageSendingTrait is used",
        "psr/http-message": "(^1.0) when HttpMessageSendingTrait is used"
    },
    "autoload": {
        "psr-4" : {
            "Subscribo\\Omnipay\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
