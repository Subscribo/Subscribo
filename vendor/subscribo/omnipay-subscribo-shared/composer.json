{
    "name": "subscribo/omnipay-subscribo-shared",
    "description": "Package with shared functionality for some Omnipay drivers",
    "keywords": [
        "Omnipay",
        "driver"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner",
            "homepage": "http://kochabo.com"
        }
    ],
    "require": {
        "omnipay/common": "^2.3.2",
        "egeloen/http-adapter": "^0.8@dev",
        "psr/http-message": "^0.10.1"
    },
    "require-dev": {
        "omnipay/tests": "^2.0",
        "phpunit/phpunit": "<5.0",
        "psr/log": "^1.0",
        "symfony/http-foundation": "<=3.0",
        "subscribo/psr-http-tools": "^0.1",
        "phly/http": "^0.12"
    },
    "suggest": {
        "subscribo/psr-http-tools": "For PSR-7 compliant Request object factory",
        "psr/log": "When GuzzleClientHelper or AbstractGateway::attachPsrLogger() is used"
    },
    "autoload": {
        "psr-4" : {
            "Subscribo\\Omnipay\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
