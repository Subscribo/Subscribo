    "keywords": [
        "laravel",
        "subscribo",
        "proxy",
        "rest",
        "http",
        "client",
        "server",
        "api"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "subscribo/config": "@dev",
        "subscribo/restclient": "@dev"
    },
    "suggest": {
