{
    "name": "subscribo/omnipay-payunity",
    "repositories": [ {"type": "composer", "url": "http://satis.localhost"} ],
    "description": "PayUnity driver for Omnipay library",
    "keywords": [
        "PayUnity",
        "Omnipay",
        "payment"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "omnipay/common": "~2.0@stable",
        "subscribo/omnipay-subscribo-shared": "@dev",
        "subscribo/psr-http-tools": "@dev",
        "psr/http-message": "*@dev"
    },
    "require-dev": {
        "omnipay/tests": "~2.0@stable"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\PayUnity\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
