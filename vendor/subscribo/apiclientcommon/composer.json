    "keywords": [
        "laravel",
        "subscribo",
        "api",
        "client",
        "common"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "KochAbo GmbH",
            "role": "owner"
        }
    ],
    "require": {
