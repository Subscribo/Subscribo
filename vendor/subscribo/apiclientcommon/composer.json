        "subscribo/restcommon": "@dev",
        "subscribo/restclient": "@dev",
        "subscribo/support": "@dev",
        "illuminate/foundation": "~5.0",
        "illuminate/contracts": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/routing": "~5.0",
        "illuminate/view": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/session": "~5.0"
