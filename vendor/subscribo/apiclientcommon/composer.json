    "autoload": {
        "psr-4": {
            "Subscribo\\ApiClientCommon\\": "src/Subscribo/ApiClientCommon/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
