{
    "name": "subscribo/config",
    "description": "Config package for reading configuration",
    "keywords": [
        "laravel",
        "subscribo",
        "config"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "KochAbo GmbH",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "symfony/config": "~2.6@stable",
        "symfony/yaml": "~2.6@stable",
        "subscribo/support": "@dev"
    },
    "suggest": {
        "laravel/framework": "This package have been developed to work with The Laravel Framework 5.0",
        "subscribo/environment": "If Environment based configuration support is needed or if using ServiceProvider. Suggested version: @dev"
    },
    "autoload": {
        "psr-4": {
            "Subscribo\\Config\\": "src/Subscribo/Config/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
