    "autoload": {
        "psr-4": {
            "Subscribo\\ApiClientLocalization\\": "src/Subscribo/ApiClientLocalization/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
