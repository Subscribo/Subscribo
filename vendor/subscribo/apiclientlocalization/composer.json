    "keywords": [
        "laravel",
        "subscribo",
        "client",
        "localization"
    ],
    "homepage": "http://www.subscribo.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner"
        }
    ],
    "require": {
        "php": "~5.4",
        "illuminate/foundation": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/routing": "~5.0",
        "illuminate/view": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/session": "~5.0",
        "subscribo/localization": "@dev",
        "subscribo/validationlocalization": "@dev",
        "subscribo/exception": "@dev"

